#include <iostream>
#include "Prototypes.h"
#include <string>

using namespace std;

int main() {
    RBT<int, int> rbt = RBT<int, int>();

    rbt.insert(13, 5);
    rbt.insert(8, 9);
    rbt.insert(17, 20);
    rbt.insert(25, 2);
    rbt.insert(1, -3);
    rbt.insert(11, -8);

    try 
    {
        rbt.print(1);
        cout << endl << "Delete the key \"6\": " << endl;
        rbt.remove(1);
        rbt.print(1);

        cout << "Keys: " << endl;
        rbt.print(2);
        cout << endl;

        cout << "Values: " << endl;
        rbt.print(3);
        cout << endl;
    }
    catch (out_of_range e)
    {
        cerr << e.what() << endl;
    }

    try {
        rbt.clear();
    }
    catch (out_of_range e)
    {
        cerr << e.what() << endl;
    }
    return 0;
}
